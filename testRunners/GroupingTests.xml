<?xml version="1.0" encoding="UTF-8"?>

<suite name="seleniumProject">
	

	<listeners>
		<listener
			class-name="com.seleniumProject.utilities.ExtentReportManager" />
	</listeners>

	<test name="seleniumProjectTests">

		<groups>
			<run>
				<include name="regression" />
				<!-- <exclude name="regression"/> -->
			</run>
		</groups>

		<parameter name="browser" value="chrome" />
		<packages>
			<package name="com.seleniumProject.testCases" />
		</packages>
	</test>

</suite>